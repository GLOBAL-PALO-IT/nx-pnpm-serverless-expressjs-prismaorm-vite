    "format:apps": "nx run-many --target=format --projects=backend,frontend",
    "format:packages": "nx run-many --target=format --projects=data,services,types,ui",
    "format:check": "nx run-many --target=format:check --all",
    "format:check:all": "nx run-many --target=format:check --all",
    "e2e": "nx e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format:write": "prettier --write .",
