{
  "name": "nx-serverless",
  "version": "1.0.0",
  "description": "Nx monorepo with serverless backend and React frontend",
  "private": true,
  "scripts": {
    "prepare": "husky",
    "build": "nx build",
    "build:all": "nx run-many --target=build --all",
    "build:backend": "nx build backend",
    "build:frontend": "nx build frontend",
    "build:packages": "nx run-many --target=build --projects=types,data,services",
    "test": "nx run-many --target=test --all",
    "test:all": "nx run-many --target=test --all",
    "test:packages": "nx run-many --target=test --projects=data,services,types,ui",
