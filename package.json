    "lint:all": "nx run-many --target=lint --all",
    "lint:apps": "nx run-many --target=lint --projects=backend,frontend",
    "lint:packages": "nx run-many --target=lint --projects=data,services,types,ui",
    "lint:backend": "nx lint backend",
    "lint:frontend": "nx lint frontend",
    "lint:fix": "nx run-many --target=lint:fix --all",
    "lint:fix:all": "nx run-many --target=lint:fix --all",
