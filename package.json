    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "dev:backend": "nx run backend:dev",
    "serve:backend": "nx serve backend",
    "package:backend": "nx run backend:package",
    "deploy:backend": "nx run backend:deploy:aws",
    "sam:build": "nx run backend:sam:build",
    "sam:deploy": "nx run backend:sam:deploy",
    "sam:deploy:guided": "nx run backend:sam:deploy:guided",
    "sam:local": "nx run backend:sam:local:api",
    "sam:invoke": "nx run backend:sam:local:invoke",
    "sls:deploy": "nx run backend:sls:deploy",
    "sls:deploy:dev": "nx run backend:sls:deploy:dev",
    "sls:deploy:prod": "nx run backend:sls:deploy:prod",
    "sls:offline": "nx run backend:sls:offline",
    "sls:dev": "nx run backend:sls:dev",
    "sls:remove": "nx run backend:sls:remove",
    "dev:frontend": "nx serve frontend",
    "test:backend": "nx test backend",
    "test:frontend": "nx test frontend",
    "db:push": "nx run data:prisma:push",
    "db:generate": "nx run data:prisma:generate",
    "db:migrate": "nx run data:prisma:migrate",
    "db:studio": "nx run data:prisma:studio",
    "db:seed": "nx run data:prisma:seed",
    "clean": "pnpm clean:build && pnpm clean:cache && pnpm clean:modules",
    "clean:build": "rm -rf dist tmp apps/*/dist apps/*/build packages/*/dist packages/*/build .serverless .aws-sam",
    "clean:cache": "nx reset && rm -rf .nx .vite node_modules/.cache",
    "clean:modules": "rm -rf node_modules apps/*/node_modules packages/*/node_modules",
    "clean:all": "pnpm clean && rm -rf pnpm-lock.yaml"
  },
  "keywords": [
    "nx",
    "monorepo",
    "serverless",
    "react",
    "typescript"
  ],
